<h4>ログインページ</h4>
<%= form_with scope: :session,url: login_path do |f| %>
<div class="form-group">
    <div class="form-group">
        <label>ユーザーID</label>
        <%= f.text_field :register_id,class:"form-control" %>
    </div>
    <div class="form-group">
        <label>パスワード</label>
        <%= f.password_field :password,class:"form-control" %>
    </div>
    <div class="form-group">
        <%= f.submit "ログインする",class:"btn btn-info" %>
    </div>
</div>
<% end %>